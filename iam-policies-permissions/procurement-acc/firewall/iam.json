{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "IamWildCard",
      "Effect": "Allow",
      "Action": [
        "iam:ListInstanceProfiles"
      ],
      "Resource": [
        "*"
      ]
    },
    {
      "Sid": "IamRole",
      "Effect": "Allow",
      "Action": [
        "iam:PassRole"
      ],
      "Resource": [
        "arn:aws:iam::017642882492:role/cloudos-fortigate-instance-role"
      ]
    }
  ]
}