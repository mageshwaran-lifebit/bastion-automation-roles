{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ElasticIp",
      "Effect": "Allow",
      "Action": [
        "ec2:AllocateAddress",
        "ec2:ReleaseAddress"
      ],
      "Resource": [
        "arn:aws:ec2:us-east-1:017642882492:elastic-ip/*"
      ]
    },
    {
      "Sid": "ElasticIp",
      "Effect": "Allow",
      "Action": [
        "ec2:AssociateAddress",
        "ec2:DisassociateAddress"
      ],
      "Resource": [
        "arn:aws:ec2:us-east-1:017642882492:elastic-ip/*",
        "arn:aws:ec2:us-east-1:017642882492:instance/*",
        "arn:aws:ec2:us-east-1:017642882492:network-interface/*"
      ]
    }
  ]
}