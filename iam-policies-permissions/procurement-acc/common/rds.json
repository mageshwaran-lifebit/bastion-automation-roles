{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "RdsTags",
      "Effect": "Allow",
      "Action": [
        "rds:ListTagsForResource",
        "rds:AddTagsToResource",
        "rds:RemoveTagsFromResource"
      ],
      "Resource": [
        "*"
      ]
    },
    {
      "Sid": "ListWildCardOnly",
      "Effect": "Allow",
      "Action": [
        "rds:DescribeDBInstances",
        "rds:DescribeDBClusters"
      ],
      "Resource": [
        "*"
      ]
    },
    {
      "Sid": "DBInstanceCreate",
      "Effect": "Allow",
      "Action": [
        "rds:CreateDBInstance"
      ],
      "Resource": [
        "arn:aws:rds:us-east-1:017642882492:cluster:cloudos",
        "arn:aws:rds:us-east-1:017642882492:db:cloudos",
        "arn:aws:rds:us-east-1:017642882492:pg:*",
        "arn:aws:rds:us-east-1:017642882492:og:*",
        "arn:aws:rds:us-east-1:017642882492:subgrp:*",
        "arn:aws:rds:us-east-1:017642882492:secgrp:*"
      ],
      "Condition": {
        "Bool": {
          "rds:ManageMasterUserPassword": true
        }
      }
    },
    {
      "Sid": "DBInstanceModify",
      "Effect": "Allow",
      "Action": [
        "rds:ModifyDBInstance"
      ],
      "Resource": [
        "arn:aws:rds:us-east-1:017642882492:db:cloudos",
        "arn:aws:rds:us-east-1:017642882492:pg:*",
        "arn:aws:rds:us-east-1:017642882492:og:*",
        "arn:aws:rds:us-east-1:017642882492:subgrp:*",
        "arn:aws:rds:us-east-1:017642882492:secgrp:*"
      ]
    },
    {
      "Sid": "DBInstanceDelete",
      "Effect": "Allow",
      "Action": [
        "rds:DeleteDBInstance"
      ],
      "Resource": [
        "arn:aws:rds:us-east-1:017642882492:db:cloudos"
      ]
    },
    {
      "Sid": "SubnetGrp",
      "Effect": "Allow",
      "Action": [
        "rds:DescribeDBSubnetGroups",
        "rds:CreateDBSubnetGroup",
        "rds:ModifyDBSubnetGroup",
        "rds:DeleteDBSubnetGroup"
      ],
      "Resource": "arn:aws:rds:us-east-1:017642882492:subgrp:*"
    },
    {
      "Sid": "KmsTag",
      "Effect": "Allow",
      "Action": [
        "kms:ListResourceTags",
        "kms:TagResource",
        "kms:UntagResource"
      ],
      "Resource": "*"
    },
    {
      "Sid": "Kms",
      "Effect": "Allow",
      "Action": [
        "kms:DescribeKey",
        "kms:CreateKey",
        "kms:GetKeyPolicy",
        "kms:ScheduleKeyDeletion",
        "kms:EnableKeyRotation",
        "kms:GetKeyRotationStatus",
        "kms:Decrypt",
        "kms:GenerateDataKey",
        "kms:CreateGrant"
      ],
      "Resource": "*"
    },
    {
      "Sid": "KmsAlias",
      "Effect": "Allow",
      "Action": [
        "kms:ListAliases",
        "kms:CreateAlias",
        "kms:DeleteAlias",
        "kms:UpdateAlias"
      ],
      "Resource": "*"
    },
    {
      "Sid": "SecretManagers",
      "Effect": "Allow",
      "Action": [
        "secretsmanager:CreateSecret",
        "secretsmanager:PutSecretValue",
        "secretsmanager:DescribeSecret",
        "secretsmanager:GetSecretValue",
        "secretsmanager:ListSecrets",
        "secretsmanager:DeleteSecret",
        "secretsmanager:TagResource",
        "secretsmanager:RotateSecret"
      ],
      "Resource": "*"
    }
  ]
}